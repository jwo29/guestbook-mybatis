<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.january.guestbook.mapper.MemberMapper">

    <insert id="save" useGeneratedKeys="true" keyProperty="mno">
        INSERT INTO members (email, password, name, reg_date, mod_date)
        VALUES (#{email}, #{password}, #{name}, now(), now());
    </insert>
</mapper>
