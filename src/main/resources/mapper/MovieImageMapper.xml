<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.january.guestbook.mapper.MovieImageMapper">
    <insert id="insert" useGeneratedKeys="true" keyProperty="inum">
        INSERT INTO movie_images (uuid, img_name, movie_mno)
        VALUES (#{uuid}, #{imgName}, #{movie.mno});
    </insert>
</mapper>
